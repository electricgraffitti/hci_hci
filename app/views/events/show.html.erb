<p>
  <b>Title:</b>
  <%=h @event.title %>
</p>

<p>
  <b>Description:</b>
  <%=h @event.description %>
</p>

<p>
  <b>Keywords:</b>
  <%=h @event.keywords %>
</p>

<p>
  <b>Permalink:</b>
  <%=h @event.permalink %>
</p>
<% if current_user %>
<%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'New', new_event_path %> |
<% end %>
<%= link_to "Back", request.env["HTTP_REFERER"] %>