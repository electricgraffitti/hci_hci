<% if @service.keywords
  meta_keywords(@service.keywords)
  end %>
<% if @service.meta_description
  meta_description(@service.meta_description)
  end %>
<% page_title "#{h(@service.title)}"%>

<% content_for :banner do %>
  <%= render :partial => "services/banner", :locals => {:service => @service}%>
  <%= render :partial => "layouts/cta_bar" %>
  <div class="divider2"></div>
<% end %>
<%= render :partial => "show_service", :locals => {:service => @service} %>
<%= render :partial => "show_quote", :locals => {:service => @service} %>
<%= render :partial => "pro_sol", :locals => {:service => @service} %>



<div class="clear"></div>
  <% if current_user %>
  <div class="grid_12 full_wrap">
    <%= link_to 'New', new_service_path %> |
    <%= link_to 'Edit', edit_service_path(@service) %>
    </div>
<% end %>
