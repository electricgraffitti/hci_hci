<% form_for(@ticket_update) do |f| %>
  <%= f.error_messages %>
    <%= hidden_field :ticket_id, @ticket_update.ticket.id %>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_area :comment %>
  </p>
  <p>
    <%= f.label :employee_id %><br />
    <%= f.text_field :employee_id %>
  </p>

  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>