<h1>Listing ticket_updates</h1>

<table>
  <tr>
    <th>Comment</th>
    <th>Employee</th>
    <th>Ticket</th>
  </tr>

<% @ticket_updates.each do |ticket_update| %>
  <tr>
    <td><%=h ticket_update.comment %></td>
    <td><%=h ticket_update.employee_id %></td>
    <td><%=h ticket_update.ticket_id %></td>
    <td><%= link_to 'Show', ticket_update %></td>
    <td><%= link_to 'Edit', edit_ticket_update_path(ticket_update) %></td>
    <td><%= link_to 'Destroy', ticket_update, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New ticket_update', new_ticket_update_path %>