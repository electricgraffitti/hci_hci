<% page_title("Welcome To ") %>
<% meta_description("HealthCare Insight provides private and public sector claims payors with a comprehensive suite of clinically validated fraud prevention services") %> 

<% content_for :banner do %>
  <%= render :partial => 'health_care/banners', :locals => {:banner => @banner} %>
<% end %>

<% content_for :sub_boxes do %>
  <h2 class="page_header home_header full_wrap">Do you have a fraud problem? We have a Fraud Solution.</h2>
  <%= render :partial => 'health_care/index_sub_boxes' %>
  <%= render :partial => "health_care/index_quote" %>
<% end %>

<div id="video_div" class="overlay">
    <!-- flowplayer container --> 
    <div id="player"></div>
</div>

<% content_for :javascripts do %>
  <%= javascript_include_tag 'index_videos', "scroll" %>
  <script type="text/javascript" charset="utf-8">
    $(document).ready(function() {
      app.hideElement($(".index_banner_nav .first"));
    });
  </script>
<% end %>

