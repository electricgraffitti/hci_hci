<h1>Article Types</h1>

<table>
  <tr>
    <th>Article Type</th>
  </tr>

<% @article_types.each do |article_type| %>
  <tr>
    <td><%=h article_type.article_type %></td>
    <td><%= link_to 'View', article_type %></td>
    <td><%= link_to 'Edit', edit_article_type_path(article_type) %></td>
    <td><%= link_to 'Delete', article_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New article_type', new_article_type_path %>