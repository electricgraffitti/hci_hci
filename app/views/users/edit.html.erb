<div class="full_wrap">
  <h1>Edit Account Info</h1>
  <% form_for(@user) do |f| %>
    <%= f.error_messages %>
    <p>
      <%= f.label :username %><br />
      <%= f.text_field :username %>
    </p>
    <p>
      <%= f.label :email %><br />
      <%= f.text_field :email %>
    </p>
    <p>
      <%= f.label :password %><br />
      <%= f.text_field :password %>
    </p>
    <p>
      <%= f.submit 'Update' %>
    </p>
  <% end %>
</div>